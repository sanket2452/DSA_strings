 int cnt = 0;
        for (auto it : words) {
            int j = 0;
            int size = 0;
            for (int i = 0; i < s.size(); i++) {
                if (it[j] == s[i]) {
                    size++;
                    j++;
                }
                if (size == it.size()) {
                    cnt++;
                    break;
                }
            }
        }
        return cnt;
